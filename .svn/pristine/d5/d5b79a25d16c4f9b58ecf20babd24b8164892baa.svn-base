@CHARSET "UTF-8";

body {
	font-size: 9pt;
	font-family: 맑은 고딕;
	width: 100%;
	margin: 0;
	padding: 0;
	word-break: break-all;
	border: none;
	overflow-y: auto;
	text-align: left;
}

select {
	height: 24px;
	font-size: 9pt;
	font-family: 맑은 고딕;
	vertical-align: middle;
}

input[type=text] {
	font-size: 9pt;
	font-family: 맑은 고딕;
	height: 18px;
	vertical-align: middle;
}

input[type=file] {
	font-size: 9pt;
	font-family: 맑은 고딕;
	height: 24px;
	vertical-align: middle;
}

input[type=checkbox] {
	font-size: 9pt;
	font-family: 맑은 고딕;
	height: 16px;
	vertical-align: middle;
	border: 0px
}

input[type=radio] {
	border: 0;
	height: 20px;
	vertical-align: middle;
}

textarea {
	font-size: 9pt;
	font-family: 맑은 고딕;
}

a {
	font-size: 1em;
	color: #333333;
	text-decoration: none;
}

a:link {
	font-size: 1em;
	color: #555555;
	text-decoration: none;
}

a:visited {
	font-size: 1em;
	color: #555555;
	text-decoration: none;
}

a:hover {
	font-size: 1em;
	color: #468BE1;
	text-decoration: underline;
}

label {
	cursor: pointer;
	color: #333333;
	text-decoration: none;
}

label:link, label:visited {
	color: #333333;
	text-decoration: none;
}

label:hover, label:active, label:focus {
	color: #468BE1;
}

.commonSeries {
	padding-left: 4px;
	padding-top: 4px;
	border: 1px solid #999999;
	height: 28px;
}

.header {
	position: relative;
	min-width: 1490px;
	height: 60px;
	margin: 0 0 10px 0px;
	width: expression(this.width < 1490 ? 1490 : true);
}

.pendingheader {
	position: relative;
	min-width: 1390px;
	height: 70px;
	margin: 0 0 10px 0px;
	width: expression(this.width < 1380 ? 1490 : true);
}

.pendingpopupheader {
	position: relative;
	width: 98%;
	height: 70px;
	margin: 0 0 10px 0px;
}

.popupheader {
	position: relative;
	width: 98%;
	height: 70px;
	margin: 0 0 10px 0px;
}
