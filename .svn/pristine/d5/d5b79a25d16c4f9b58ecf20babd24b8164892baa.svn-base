.loadingBar {
	position: relative;
	text-align: center;
	height: 30px;
	margin-top: 6px;
	display: none;
	width: 100%;
	clear: both;
}

.loadingBoxArea {
	position: fixed;
	height: 100%;
	width: 100%;
	left: 0;
	top: 0;
	z-index: 10;
	z-index: 199;
	display: none;
}

.loadingBg {
	position: fixed;
	height: 100%;
	width: 100%;
	left: 0;
	top: 0;
	filter: alpha(opacity : '30');
	background-color: black;
	z-index: 1;
}

.loadingProgress {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background-image: url(/ematrix/TBC/images/ajax-loader-5.gif);
	background-repeat: no-repeat;
	background-position: center;
	z-index: 3;
}

.loadingText {
	position: fixed;
	width: 500px;
	height: 130px;
	z-index: 2;
	text-align: center;
	background-color: #FFFFFF;
	left: 50%;
	top: 50%;
	margin-left: -265px;
	margin-top: 30px;
	border: 3px solid #DA010A;
	padding: 20px;
	font-size: 11pt;
}

.blue-loader .ajax_loader {
	/* background: url("../images/ajax-loader-3.gif") no-repeat center center transparent; */
	position: absolute;
	top: 45%;
	left: 45%;
	height: 50px;
	border: 2px solid #36b1fc;
	z-index: 999999;
}

.commonSeries {
	padding-left: 4px;
	padding-top: 4px;
	border: 1px solid #999999;
	height: 28px;
}

.mainBodyScroll {
	overflow-x: auto;
	width: 100%;
}

.pagingArea {
	width: 100%;
	text-align: center;
	margin-top: 10px;
}

.bomTotalCntArea {
	font-weight: bold;
}

.bomTotalCnt {
	color: red;
	font-size: 14pt;
	font-weight: bold;
}

.ui-autocomplete {
	max-height: 200px;
	overflow-y: auto;
	/* prevent horizontal scrollbar */
	overflow-x: hidden;
}

.tooltipArea {
	position: absolute;
	width: auto;
	height: auto;
	border: 2px solid #666;
	background-color: #ffffff;
	font-weight: bold;
	color: #666;
	display: none;
	padding: 5px;
	box-shadow: 2px 2px 4px #888;
	border-radius: 4px;
}

.grid_warning {
	background-color: #F9C474
}

/*레이어 관련*/
.layer {
	display: none;
	position: fixed;
	_position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 99;
}

.layer .bg {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #000;
	opacity: .5;
	filter: alpha(opacity = 50);
}

.layer .pop-layer {
	display: block;
}

.pop-layer {
	display: none;
	position: absolute;
	top: 50%;
	left: 50%;
	width: 410px;
	height: auto;
	background-color: #fff;
	border: 5px solid #3571B5;
	z-index: 100;
}

.pop-layer .pop-container {
	padding: 20px 25px;
}

.pop-layer p.ctxt {
	color: #666;
	line-height: 25px;
}

.pop-layer .btn-r {
	width: 100%;
	margin: 10px 0 20px;
	padding-top: 10px;
	border-top: 1px solid #DDD;
	text-align: right;
}

a.cbtn {
	display: inline-block;
	height: 25px;
	padding: 0 14px 0;
	border: 1px solid #304a8a;
	background-color: #3f5a9d;
	font-size: 13px;
	color: #fff;
	line-height: 25px;
}

a.cbtn:hover {
	border: 1px solid #091940;
	background-color: #1f326a;
	color: #fff;
}

